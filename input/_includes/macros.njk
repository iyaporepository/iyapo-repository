{% macro test(msg) %}
<div>{{msg}}</div>
{% endmacro %}

{% macro bevel_box(sides,corners) %}
<div class="bevel-box {{sides}}">
        {% for corner in corners %}
            <div class='corner {{corner}}'></div>
        {% endfor %}
    </div>
{% endmacro %}


